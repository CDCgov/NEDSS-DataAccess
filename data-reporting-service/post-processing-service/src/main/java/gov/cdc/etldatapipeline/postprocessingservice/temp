def snake_to_camel(snake_str):
    components = snake_str.split('_')
    # Capitalize all words except the first one
    camel_case = components[0].lower() + ''.join(x.title() for x in components[1:])
    return camel_case

# Read data from the text file
input_file_path = '/Users/RagulShanmugam/Documents/Workspace/Misc/temp.txt'  # Replace 'data.txt' with the path to your input text file
output_file_path = '/Users/RagulShanmugam/Documents/Workspace/Misc/converted_data.txt'  # Path to the output text file
with open(input_file_path, 'r') as input_file:
    lines = input_file.readlines()

# Convert each variable name to camelCase and write them to the output file
with open(output_file_path, 'w') as output_file:
    for line in lines:
        snake_case_variable = line.strip()  # Remove leading/trailing whitespace and newline character
        camel_case_variable = snake_to_camel(snake_case_variable)
        output_file.write(@Column(name = " + snake_case_variable.lower() + ") + '\n')
        output_file.write("private String " + camel_case_variable + '\n')

print("Conversion complete. Output written to:", output_file_path)

# Close the files
input_file.close()
output_file.close()
